<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="containerPane" prefHeight="600.0" prefWidth="1100.0" style="-fx-background-color: #75dfff;" stylesheets="@css/style.css" xmlns="http://javafx.com/javafx/null" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.ecanteen.ecanteen.controllers.DetailSupplierController">
    <HBox alignment="CENTER" layoutY="10.0" AnchorPane.leftAnchor="193.0" AnchorPane.rightAnchor="20.0" AnchorPane.topAnchor="10.0">
        <VBox alignment="CENTER" spacing="5.0" HBox.hgrow="ALWAYS">
            <Label alignment="CENTER" contentDisplay="CENTER" text="PRODUK YANG DIKIRIM" textAlignment="CENTER">
                <font>
                    <Font name="System Bold" size="18.0" />
                </font>
            </Label>
            <Label fx:id="supplierNameLabel" alignment="CENTER" contentDisplay="CENTER" text="Oleh : " textAlignment="CENTER">
                <font>
                    <Font name="System Bold" size="16.0" />
                </font>
            </Label>
        </VBox>
        <Button fx:id="backButton" mnemonicParsing="false" onAction="#backAction" prefHeight="40.0" styleClass="reset" text="KEMBALI" textAlignment="CENTER" textFill="WHITE">
            <font>
                <Font name="System Bold" size="14.0" />
            </font>
            <padding>
                <Insets left="25.0" right="25.0" />
            </padding>
        </Button>
    </HBox>
    <TableView fx:id="detailSupplierTableView" layoutX="300.0" layoutY="160.0" AnchorPane.bottomAnchor="20.0" AnchorPane.leftAnchor="20.0" AnchorPane.rightAnchor="20.0" AnchorPane.topAnchor="77.0">
      <columns>
          <TableColumn fx:id="noTableColumn" maxWidth="50.0" minWidth="50.0" prefWidth="-1.0" sortable="false" styleClass="column-center" text="No" />
          <TableColumn fx:id="barcodeTableColumn" prefWidth="75.0" text="Barcode" />
          <TableColumn fx:id="nameTableColumn" prefWidth="75.0" text="Nama Barang" />
          <TableColumn fx:id="purchasePriceTableColumn" maxWidth="130.0" minWidth="130.0" prefWidth="130.0" styleClass="column-right" text="Harga Beli" />
          <TableColumn fx:id="sellingPriceTableColumn" maxWidth="130.0" minWidth="130.0" prefWidth="130.0" styleClass="column-right" text="Harga Jual" />
          <TableColumn fx:id="stockAmountTableColumn" maxWidth="130.0" minWidth="130.0" prefWidth="130.0" styleClass="column-center" text="Jumlah Stok" />
          <TableColumn fx:id="categoryTableColumn" prefWidth="75.0" text="Kategori" />
      </columns>
        <columnResizePolicy>
          <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
        </columnResizePolicy>
    </TableView>
</AnchorPane>
